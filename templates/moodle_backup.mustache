<?xml version="1.0" encoding="UTF-8"?>
<moodle_backup>
    <information>
        <name>{{name}}</name>
        <moodle_version>{{moodle.version}}</moodle_version>
        <moodle_release>{{moodle.release}}</moodle_release>
        <backup_version>{{backup.version}}</backup_version>
        <backup_release>{{backup.release}}</backup_release>
        <backup_date>{{backup.date}}</backup_date>
        <mnet_remoteusers>0</mnet_remoteusers>
        <include_files>1</include_files>
        <include_file_references_to_external_content>0</include_file_references_to_external_content>
        <original_wwwroot>{{original_wwwroot}}</original_wwwroot>
        <original_site_identifier_hash>{{original_site_identifier_hash}}</original_site_identifier_hash>
        <original_course_id>{{original_course_id}}</original_course_id>
        <original_course_format>{{original_course_format}}</original_course_format>
        <original_course_fullname>{{original_course_fullname}}</original_course_fullname>
        <original_course_shortname>{{original_course_shortname}}</original_course_shortname>
        <original_course_startdate>{{original_course_startdate}}</original_course_startdate>
        <original_course_enddate>{{original_course_enddate}}</original_course_enddate>
        <original_course_contextid>{{original_course_contextid}}</original_course_contextid>
        <original_system_contextid>{{original_system_contextid}}</original_system_contextid>
        <details>
            <detail backup_id="{{backup.id}}">
                <type>course</type>
                <format>moodle2</format>
                <interactive>{{backup.interactive}}</interactive>
                <mode>{{backup.mode}}</mode>
                <execution>{{backup.execution}}</execution>
                <executiontime>{{backup.executiontime}}</executiontime>
            </detail>
        </details>
        <contents>
            <activities>
                {{#activities}}
                <activity>
                    <moduleid>{{moduleid}}</moduleid>
                    <sectionid>{{sectionid}}</sectionid>
                    <modulename>{{modulename}}</modulename>
                    <title>{{title}}</title>
                    <directory>{{directory}}</directory>
                </activity>
                {{/activities}}
            </activities>
            <sections>
                {{#sections}}
                <section>
                    <sectionid>{{id}}</sectionid>
                    <title>{{title}}</title>
                    <directory>{{directory}}</directory>
                </section>
                {{/sections}}
            </sections>
            <course>
                <courseid>{{course.id}}</courseid>
                <title>{{course.title}}</title>
                <directory>course</directory>
            </course>
        </contents>
        <settings>
            {{#settings}}
            <setting>
                <level>{{level}}</level>
                {{#additionaltag}}{{{additionaltag}}}{{/additionaltag}}
                <name>{{name}}</name>
                <value>{{value}}</value>
            </setting>
            {{/settings}}
        </settings>
    </information>
</moodle_backup>